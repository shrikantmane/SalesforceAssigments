        <description>Enter the Resignation Date if Notice Period checked to true</description>
        <errorConditionFormula>Is_on_Notice_Period__c &amp;&amp;  ISBLANK( Resignation_Date__c )</errorConditionFormula>
        <errorDisplayField>Resignation_Date__c</errorDisplayField>
        <errorMessage>Resignation Date is mandatory if employee is on notice period</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SSN_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>IF(LEN( TEXT( SSN__c )) &gt; 10, true, false)</errorConditionFormula>
        <errorDisplayField>SSN__c</errorDisplayField>
        <errorMessage>SSN should not be more than 10 digits</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Employee_Age</fullName>
        <active>true</active>
        <errorConditionFormula>Age__c  &gt;= 30 &amp;&amp; Age__c  &lt;= 21</errorConditionFormula>
